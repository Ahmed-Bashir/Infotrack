@model IEnumerable<Scrape>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}




<table class="table">
    <thead>
        <tr>
            <th scope="col">Rank</th>
            <th scope="col">Link</th>
        
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">
                    @item.Rank
                </th>
                 
             
                <td>
                    @item.Link
                </td>
            </tr>
        }
    </tbody>
</table>
